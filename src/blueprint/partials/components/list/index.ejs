<% 
    var __list_type__ = typeof list_type != 'undefined' ? list_type : 'ul';
    var list_items = typeof list_items != 'undefined' ? list_items : '';
    
    if(typeof list_items == 'object' && Array.isArray(list_items)) {
        list_items = list_items.map(item => require('/blueprint/partials/components/list/item.ejs')(item)).join('');
    }

    var __attributes__ = [
        typeof class_names != 'undefined' ? `class="${class_names}"` : '',
    ].join(' ');    
%>
<<%- __list_type__ %> <%- __attributes__ %>>
    <%- list_items %>
</<%- __list_type__ %>>