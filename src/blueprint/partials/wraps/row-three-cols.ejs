<% 
    var cols = typeof cols !== "undefined" ? cols : [];
    var col_1_content = typeof cols[0].content !== "undefined" ? cols[0].content : "";
    var col_2_content = typeof cols[1] !== "undefined" ? cols[1].content : "";
    var col_3_content = typeof cols[2] !== "undefined" ? cols[2].content : "";
%>
<div class="row g-3">
    <div class="col-12 col-md-4">
        <%- col_1_content %>
    </div>
    <div class="col-12 col-md-4">
        <%- col_2_content %>
    </div>
    <div class="col-12 col-md-4">
        <%- col_3_content %>  
    </div>
</div>